{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "width": 500,
    "height": 400,
    "title": "COVID-19 Cases per Country (13 Oct 2020)",
    "data": {
        "url": "https://raw.githubusercontent.com/vincenttaneli/FIT3179_A2/main/A2/data/data_grouped.csv"
    },
    "mark": "circle",
    "encoding": {
        "x": {
            "field": "Crash Rate per 100,000 population",
            "type": "quantitative",
            "title": "Crash rate"
        },
        "y": {
            "field": "Fatality Rate per 100,000 population",
            "type": "quantitative",
            "title": "Fatality rate"
        },
        "color": {
            "field": "State",
            "type": "nominal"
        },
        "size": {
            "field": "Estimated Population",
            "type": "quantitative"
        }
    }
}