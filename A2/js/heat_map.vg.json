{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "data": {"url": "https://raw.githubusercontent.com/vincenttaneli/FIT3179_A2/main/A2/data/data_grouped_2.csv"},
    
    "mark": "rect",
    "width": 300,
    "height": 200,
    "encoding": {
      "x": {
        "field": "Dayweek",
        "type": "ordinal"
      },
      "y": {
        "field": "Month",
        "type": "ordinal"
      },
      "color": {
        "field": "Crash_Count",
        "type": "quantitative"
      }
    },
    "config": {
      "view": {
        "stroke": "transparent"
      }
    }
  }
  