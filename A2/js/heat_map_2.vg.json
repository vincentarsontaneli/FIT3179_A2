{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "data": {"url": "https://raw.githubusercontent.com/vincenttaneli/FIT3179_A2/main/A2/data/data_grouped_3.csv"},
    
    "mark": "rect",
    "width": 200,
    "height": 650,
    "encoding": {
      "y": {
        "field": "Hour",
        "type": "ordinal",
        "axis": {"labelAngle": 0}
      },
      "x": {
        "field": "Dayweek Initial",
        "type": "ordinal",
        "sort": ["M", "T", "W", "Th", "F", "S", "Su"],
        "title": "Day",
        "axis": {"labelAngle": 0}
      },
      "color": {
        "field": "Crash Count",
        "type": "quantitative"
      },
      "tooltip": [
        {"field": "Dayweek", "type": "ordinal", "title": "Day"},
        {"field": "Hour", "type": "ordinal", "format": ","},
        {"field": "Crash Count", "type": "quantitative", "format": ","}
        ]
    },
    "config": {
      "view": {
        "stroke": "transparent"
      }
    }
  }
  